<h3>Discover Movies</h3>

<%= button_to 'Find Top Rated Movies', user_movies_path(@user), params: { q: 'top 20rated' }, method: :get %>

<%= form_with url: user_movies_path(@user), params: { q: 'keyword' }, method: :get, local: true do |form| %>
  <%= form.text_field :title %>
  <%= form.submit "Find Movies" %>
<% end %>